{
    "functions": {
        "api/api.php": {
            "runtime": "vercel-php@0.7.3"
        }
    },
    "routes": [
        {
            "src": "/api/api.php",
            "dest": "/api/api.php"
        },
        {
            "src": "/(.*)",
            "dest": "/api/api.php"
        }
    ]
}
